{
  "title": "SP â€” JVM",
  "uid": "sp-jvm",
  "tags": ["sp","jvm"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "refresh": "5s",
  "time": { "from": "now-1h", "to": "now" },
  "links": [
    { "type": "dashboards", "asDropdown": true, "title": "SP Dashboards", "tags": ["sp"], "includeVars": true, "keepTime": true }
  ],
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "datasource": "Prometheus",
        "includeAll": true,
        "refresh": 2,
        "query": "label_values(process_cpu_usage, instance)"
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Heap used",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "sum without (id) (jvm_memory_used_bytes{area=\"heap\",instance=~\"$instance\"})", "legendFormat": "{{instance}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Non-heap used",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "sum without (id) (jvm_memory_used_bytes{area=\"nonheap\",instance=~\"$instance\"})", "legendFormat": "{{instance}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Alloc rate (bytes/s)",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "rate(jvm_gc_memory_allocated_bytes_total{instance=~\"$instance\"}[$__rate_interval])", "legendFormat": "{{instance}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] },
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "GC pause p95 (ms)",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "1000 * histogram_quantile(0.95, sum(rate(jvm_gc_pause_seconds_bucket{instance=~\"$instance\"}[$__rate_interval])) by (le,instance))", "legendFormat": "{{instance}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Live vs Max OldGen (%)",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "100 * (jvm_gc_live_data_size_bytes{instance=~\"$instance\"} / jvm_gc_max_data_size_bytes{instance=~\"$instance\"})", "legendFormat": "{{instance}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Threads live",
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "jvm_threads_live_threads{instance=~\"$instance\"}", "legendFormat": "{{instance}}" }
      ],
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 }
    }
  ]
}
